{
  "name": "example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "metro": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "cd web && webpack-dev-server --host 0.0.0.0",
    "build:web": "rm -rf dist/ && cd web && webpack --progress --hide-modules",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "destiny": "file:../lib",
    "react": "16.9.0",
    "react-dom": "^16.13.0",
    "react-native": "0.62.3",
    "react-native-dark-mode": "^0.2.2",
    "react-native-localize": "^1.3.4",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-web": "^0.12.2",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.24",
    "@types/react-native": "^0.60.25",
    "@types/react-test-renderer": "16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-native-web": "^0.12.2",
    "eslint": "^5.16.0",
    "html-webpack-plugin": "^3.2.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.9.0",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "semi": [
        2,
        "never"
      ]
    }
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": false,
    "jsxBracketSameLine": true,
    "singleQuote": true,
    "trailingComma": "all",
    "semi": false
  }
}
